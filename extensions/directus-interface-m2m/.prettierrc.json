{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 100,
  "requirePragma": false,
  "proseWrap": "preserve",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css"
}
